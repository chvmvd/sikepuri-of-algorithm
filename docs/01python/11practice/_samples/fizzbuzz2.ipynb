    "def fizzbuzz(n):\n",
    "    for i in range(1, n + 1):\n",
    "        s = \"\"\n",
    "        if i % 3 == 0:\n",
    "            s += \"Fizz\"\n",
    "        if i % 5 == 0:\n",
    "            s += \"Buzz\"\n",
    "        if s == \"\":\n",
    "            s = i\n",
    "        print(s)\n",
    "\n",
    "\n",
    "fizzbuzz(30)"
   ],
